<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GetMatch</servlet-name>
        <servlet-class>servlet.GetMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetIndividual</servlet-name>
        <servlet-class>servlet.GetIndividual</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetMatch</servlet-name>
        <url-pattern>/GetMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetIndividual</servlet-name>
        <url-pattern>/GetIndividual</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
